{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "dependsOn": ["^db:push"]
    },
    "db:generate": {
      "dependsOn": ["^db:generate"]
    },
    "db:seed": {
      "dependsOn": ["^db:seed"]
    },
    "db:reset": {
      "dependsOn": ["^db:reset"]
    },
    "benchmark": {
      "dependsOn": ["^benchmark", "^build"],
      "cache": false
    }
  }
}
